{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../build"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Forge Command",
  "version": "0.1.0",
  "identifier": "com.forge.command",
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Forge Command",
        "width": 1400,
        "height": 900,
        "minWidth": 1200,
        "minHeight": 700,
        "decorations": true,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
